  <modelVersion>4.0.0</modelVersion>
  <groupId>hu.prolan</groupId>
  <artifactId>hu.prolan.viatraTest</artifactId>
  <version>1.2.3</version>
  <repositories>
    <repository>
      <snapshots>
        <enabled>false</enabled>
